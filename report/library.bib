doi = {10.1016/j.neucom.2014.10.066},
file = {:C$\backslash$:/visual{\_}object{\_}tracking/report/citeFiles/Sarailoo, Rahmani, Rezaie - A novel model predictive control scheme based on bees algorithm in a class of nonlinear systems Application.pdf:pdf},
