@phdthesis{BrinanMirt1997,
abstract = {This paper presents the Voronoi-clip,or V-Clip,collision detection algorithm for polyhedral objects speci?edby a boundary representation. V-Clip tracks the closest pair of features between convex polyhedra, using an approach reminiscent of the Lin-Canny closest features algorithm. V-Clip is an improvement over the latter in several respects. Coding complexity is reduced, and robustness is signi?cantly improved; the implementation has no numerical tolerances and does not exhibit cycling problems. The algorithm also handles penetrating polyhedra, making it useful for nonconvex polyhedral collision detection. This paper presents the theoretical principles of V-Clip, and gives a pseudocode description of the algorithm. It also documents various tests that compare V-Clip, Lin-Canny, and the Enhanced GJK algorithm, a simplex-based algorithm that is widely used for the same application. The results show V-Clip to be a strong contender in this ?eld, comparing favorably with the other algorithms in most of the tests, in terms of both performanceand robustness},
author = {{Brinan Mirt}},
file = {:C$\backslash$:/visual{\_}object{\_}tracking/report/citeFiles/Brinan Mirt - V-Clip Fast and Robust Polyhedral Collision Detection - 1997.pdf:pdf},
title = {{V-Clip: Fast and Robust Polyhedral Collision Detection}},
year = {1997}
}
@article{Yoon2008,
abstract = {This paper presents a model predictive approach for obstacle avoidance of car-like unmanned ground vehicles (UGVs). An optimal tracking problem while avoiding obstacles in unknown environments is formulated in terms of cost minimization under constraints. Information on obstacles can be incorporated online in the nonlinear model predictive framework and kinematic constraints are treated by Karush-Kuhn-Tucker (KKT) condition. The overall problem is solved real-time with nonlinear programming. This approach is applied to car-like robots including tire models while explicitly considering the dimension of the UGV rather than treating it as a dimensionless cart model. Two kinds of potential-like terms are employed in the cost function for obstacles avoidance. The first term is to consider the distance between the UGV and the obstacle, and the second one is to consider the parallax information of the UGV about the obstacles. Simulation results show that both two approaches can make safe steering in a simple environment, but in a complex environment such as an urban area, the approach based on the modified parallax (MP) was more successful in the view of the computation time and safe steering. Copyright {\textcopyright} 2007 International Federation of Automatic Control All Rights Reserved.},
author = {Yoon, Yongsoon and Choe, Tokson and Park, Yongwoon and Kim, H. Jin},
doi = {10.3182/20080706-5-KR-1001.1873},
file = {:C$\backslash$:/visual{\_}object{\_}tracking/report/citeFiles/Yoon et al. - Obstacle avoidance for wheeled robots in unknown environments using model predictive control - 2008.pdf:pdf},
isbn = {9783902661005},
issn = {14746670},
journal = {IFAC Proceedings Volumes (IFAC-PapersOnline)},
keywords = {Autonomous robotic systems,Guidance navigation and control,Mobile robots},
number = {1 PART 1},
pages = {6792--6797},
title = {{Obstacle avoidance for wheeled robots in unknown environments using model predictive control}},
volume = {17},
year = {2008}
}
