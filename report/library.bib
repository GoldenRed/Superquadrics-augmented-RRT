@article{Tang2009,
abstract = {We present an interactive algorithm for continuous collision detection between deformable models. We introduce multiple techniques to improve the culling efficiency and the overall performance of continuous collision detection. First, we present a novel formulation for continuous normal cones and use these normal cones to efficiently cull large regions of the mesh as part of self-collision tests. Second, we introduce the concept of ldquoprocedural representative trianglesrdquo to remove all redundant elementary tests between nonadjacent triangles. Finally, we exploit the mesh connectivity and introduce the concept of ldquoorphan setsrdquo to eliminate redundant elementary tests between adjacent triangle primitives. In practice, we can reduce the number of elementary tests by two orders of magnitude. These culling techniques have been combined with bounding volume hierarchies and can result in one order of magnitude performance improvement as compared to prior collision detection algorithms for deformable models. We highlight the performance of our algorithm on several benchmarks, including cloth simulations, N-body simulations, and breaking objects.},
author = {Tang, Min and Curtis, Sean and Yoon, Sung Eui and Manocha, Dinesh},
doi = {10.1109/TVCG.2009.12},
file = {:C$\backslash$:/visual{\_}object{\_}tracking/report/citeFiles/Tang et al. - ICCD Interactive continuous collision detection between deformable models using connectivity-based culling - 2009.pdf:pdf},
isbn = {9781605581062},
issn = {10772626},
journal = {IEEE Transactions on Visualization and Computer Graphics},
keywords = {Bounding volume hierarchies,Continuous collision detection,Continuous normal cones,Deformable models,Orphan set,Self-collision},
number = {4},
pages = {544--557},
pmid = {19423880},
title = {{ICCD: Interactive continuous collision detection between deformable models using connectivity-based culling}},
volume = {15},
year = {2009}
}
