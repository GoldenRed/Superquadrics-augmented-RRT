issn = {09218890},
journal = {Robotics and Autonomous Systems},
keywords = {Camera pose estimation,Five point algorithm,Relinearization,Vision based estimation,Visual servoing},
number = {June},
pages = {45--62},
title = {{Camera relative pose estimation for visual servoing using quaternions}},
volume = {107},
