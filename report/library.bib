@article{Shah2012,
abstract = {Euler angles describe rotations of a rigid body in three-dimensional Cartesian space, as can be obtained by, say, a spherical joint. The rotation carried out by a spherical joint can also be expressed by using three intersecting revolute joints that can be described using the popular Denavit-Hartenberg (DH) parameters. However, the motions of these revolute joints do not necessarily correspond to any set of the Euler angles. This paper attempts to correlate the Euler angles and DH parameters by introducing a concept of DH parameterization of Euler angels. A systematic approach is presented in order to obtain the DH parameters for any Euler angles set. This gives rise to the concept of Euler-angle-joints (EAJs), which provide rotations equivalent to a particular set of Euler angles. Such EAJs can be conveniently used for the modeling of multibody systems having multiple-degrees-of-freedom joints.},
author = {Shah, S. V. and Saha, S. K. and Dutt, J. K.},
doi = {10.1115/1.4005467},
file = {:C$\backslash$:/visual{\_}object{\_}tracking/report/citeFiles/Shah, Saha, Dutt - Denavit-Hartenberg Parameterization of Euler Angles - 2012.pdf:pdf},
issn = {15551423},
journal = {Journal of Computational and Nonlinear Dynamics},
keywords = {dh parameterization,euler angles,euler-angle-,intersecting revolute joints},
number = {2},
pages = {021006},
title = {{Denavit-Hartenberg Parameterization of Euler Angles}},
volume = {7},
year = {2012}
}
@article{Volpe1990,
abstract = {Previous work in arti?cial potentials has demonstrated the need for an obstacle avoidancepo- tential that closely models the obstacle, yet does not generate local minima in the workspace of the manipulator. This paper presents a potential function based on superquadrics, which closely models a large class of object shapes. This potential function also prevents the cre- ation of local minima when it is added to spherically symmetric attractive wells. We intro- duce two compatible forms ofthe superquadric potential function: one for obstacle avoidance, and another for obstacle approach. We have implemented the avoidance and approach poten- tials in simulations. In these simulations the end e?ector of the manipulator experiences an attractive forcefrom a global spherical well, while the end e?ector and each of the links expe- riencerepulsive forces from all of the objects. We have also experimentally implemented the avoidancepotentials on the CMU DDARM II. The results demonstrate successful obstacle avoidance and approach, and exhibit an improvement over existing potential schemes.},
author = {Volpe, R and on Systems, P Khosla - IEEE Transactions and Man, Undefined and And, Undefined and 1990, Undefined},
