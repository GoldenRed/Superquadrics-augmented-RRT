journal = {Sensors (Switzerland)},
keywords = {Extended Kalman filter,Inertial sensors,Mobile robot,Object,Pose estimation,Vision},
number = {10},
pmid = {28934102},
title = {{Pose estimation of a mobile robot based on fusion of IMU data and vision data using an extended kalman filter}},
volume = {17},
year = {2017}
}
