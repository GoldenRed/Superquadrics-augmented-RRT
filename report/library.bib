@article{Gørtz2009,
abstract = {Dial a ride problems consist of a metric space (denoting travel time between vertices) and a set of m objects represented as source-destination pairs, where each object requires to be moved from its source to destination vertex. We consider the multi-vehicle Dial a ride problem, with each vehicle having capacity k and its own depot-vertex, where the objective is to minimize the maximum completion time (makespan) of the vehicles. We study the "preemptive" version of the problem, where an object may be left at intermediate vertices and transported by more than one vehicle, while being moved from source to destination. Our main results are an O(log{\^{}}3 n)-approximation algorithm for preemptive multi-vehicle Dial a ride, and an improved O(log t)-approximation for its special case when there is no capacity constraint. We also show that the approximation ratios improve by a log-factor when the underlying metric is induced by a fixed-minor-free graph.},
archivePrefix = {arXiv},
arxivId = {arXiv:1102.5450v1},
author = {G{\o}rtz, Inge Li and Nagarajan, Viswanath and Ravi, R.},
doi = {10.1007/978-3-642-04128-0_48},
eprint = {arXiv:1102.5450v1},
file = {:C$\backslash$:/visual{\_}object{\_}tracking/report/citeFiles/G{\o}rtz, Nagarajan, Ravi - Minimum makespan multi-vehicle dial-a-ride - 2009.pdf:pdf},
isbn = {3642041272},
issn = {03029743},
journal = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
pages = {540--552},
title = {{Minimum makespan multi-vehicle dial-a-ride}},
