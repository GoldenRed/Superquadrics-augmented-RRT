@article{Potthast2008,
abstract = {e goal configuration should be reachable through a collision free path. Generally, many con?gurations ful?ll a desired goal position, therefore a mo- tion planner should consider all possible solutions. In complex, cluttered envi- ronments it is important that a path planning algorithm considers more than one solution and exploits the redundancy of the manipulator. The proposed method considers multiple goal con?gurations by exploring the high dimensional con?guration space of a highly redundant manipulator with a Rapidly-Exploring Random Tree. This approach combines the search for a goal con?guration with path planning to overcome the limitations of inverse kinematic approaches. Extending the RRT according to a heuristic guides the search tree to the goal region while avoiding con?gurations that collide with obstacles. Once the goal region has been reached, a nonlinear optimization algorithm is applied to e?ciently ?nd a configuration that minimizes the distance to the goal position. A path from initial to goal con?guration can be extracted from the generated RRT. The collision free path can now be tracked in the reaching task. The proposed path planner was thoroughly evaluated in several scenarios with different difficulty levels. Within these experiments the algorithm has successfully been used to e?ciently plan reaching motions in several scenarios.},
author = {Potthast, Christian},
file = {:C$\backslash$:/visual{\_}object{\_}tracking/report/citeFiles/Potthast - Robot Manipulation Planning with Rapidly Exploring Random Trees in simulated Environments - 2008.pdf:pdf},
